{
  "name": "@paklo/runner",
  "version": "0.1.1",
  "sideEffects": false,
  "type": "module",
  "author": "mburumaxwell",
  "license": "AGPL-3.0-later",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./local": {
      "types": "./dist/local/index.d.ts",
      "import": "./dist/local/index.js"
    },
    "./local/azure": {
      "types": "./dist/local/azure.d.ts",
      "import": "./dist/local/azure.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "prebuild": "tsc",
    "build": "tsdown",
    "lint": "biome check",
    "test": "vitest",
    "clean": "rimraf .turbo dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mburumaxwell/dependabot-azure-devops.git"
  },
  "keywords": [
    "dependabot",
    "azure",
    "devops",
    "paklo"
  ],
  "bugs": {
    "url": "https://github.com/mburumaxwell/dependabot-azure-devops/issues"
  },
  "homepage": "https://github.com/mburumaxwell/dependabot-azure-devops#readme",
  "dependencies": {
    "@hono/node-server": "1.19.5",
    "@paklo/core": "workspace:*",
    "dockerode": "4.0.9",
    "node-forge": "1.3.1",
    "tar-stream": "3.1.7"
  },
  "devDependencies": {
    "@types/dockerode": "3.3.45",
    "@types/node": "24.9.1",
    "@types/node-forge": "1.3.14",
    "@types/tar-stream": "3.1.4",
    "tsdown": "0.15.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
