{
  "name": "@paklo/cli",
  "version": "0.8.3",
  "sideEffects": false,
  "type": "module",
  "author": "mburumaxwell",
  "license": "AGPL-3.0-later",
  "bin": {
    "paklo": "./dist/cli.js"
  },
  "exports": {
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./azure": {
      "types": "./dist/azure.d.ts",
      "import": "./dist/azure.js"
    },
    "./dependabot": {
      "types": "./dist/dependabot.d.ts",
      "import": "./dist/dependabot.js"
    },
    "./environment": {
      "types": "./dist/environment.d.ts",
      "import": "./dist/environment.js"
    },
    "./github": {
      "types": "./dist/github.d.ts",
      "import": "./dist/github.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js"
    },
    "./shared-data": {
      "types": "./dist/shared-data.d.ts",
      "import": "./dist/shared-data.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "postinstall": "tsup",
    "dev": "tsup --watch",
    "prebuild": "tsc",
    "build": "tsup",
    "lint": "biome check",
    "test": "vitest",
    "clean": "rimraf .turbo dist",
    "cli": "node dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mburumaxwell/dependabot-azure-devops.git"
  },
  "keywords": [
    "dependabot",
    "azure",
    "devops",
    "paklo"
  ],
  "bugs": {
    "url": "https://github.com/mburumaxwell/dependabot-azure-devops/issues"
  },
  "homepage": "https://github.com/mburumaxwell/dependabot-azure-devops#readme",
  "dependencies": {
    "@hono/node-server": "1.19.3",
    "@hono/zod-validator": "0.7.2",
    "commander": "14.0.1",
    "dockerode": "4.0.8",
    "hono": "4.9.7",
    "js-yaml": "4.1.0",
    "node-forge": "1.3.1",
    "pino": "9.9.5",
    "pino-pretty": "13.1.1",
    "semver": "7.7.2",
    "tar-stream": "3.1.7",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@types/dockerode": "3.3.43",
    "@types/js-yaml": "4.0.9",
    "@types/node-forge": "1.3.14",
    "@types/semver": "7.7.1",
    "@types/tar-stream": "3.1.4",
    "tsup": "8.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
