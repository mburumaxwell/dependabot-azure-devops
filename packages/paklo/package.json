{
  "name": "paklo",
  "version": "0.6.0",
  "sideEffects": false,
  "type": "module",
  "private": true,
  "license": "AGPL-3.0-later",
  "bin": {
    "paklo": "./dist/cli.cjs"
  },
  "exports": {
    "./azure": {
      "types": "./dist/azure.d.ts",
      "require": "./dist/azure.cjs",
      "import": "./dist/azure.js"
    },
    "./dependabot": {
      "types": "./dist/dependabot.d.ts",
      "require": "./dist/dependabot.cjs",
      "import": "./dist/dependabot.js"
    },
    "./environment": {
      "types": "./dist/environment.d.ts",
      "require": "./dist/environment.cjs",
      "import": "./dist/environment.js"
    },
    "./github": {
      "types": "./dist/github.d.ts",
      "require": "./dist/github.cjs",
      "import": "./dist/github.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "require": "./dist/logger.cjs",
      "import": "./dist/logger.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "postinstall": "tsup",
    "dev": "tsup --watch",
    "prebuild": "tsc",
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest",
    "clean": "rimraf .turbo dist",
    "cli": "node dist/cli.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mburumaxwell/dependabot-azure-devops.git"
  },
  "keywords": [
    "dependabot",
    "azure",
    "devops",
    "paklo"
  ],
  "bugs": {
    "url": "https://github.com/mburumaxwell/dependabot-azure-devops/issues"
  },
  "homepage": "https://github.com/mburumaxwell/dependabot-azure-devops#readme",
  "dependencies": {
    "@hono/node-server": "1.19.1",
    "@hono/zod-validator": "0.7.2",
    "axios": "1.11.0",
    "azure-devops-node-api": "15.1.1",
    "commander": "14.0.0",
    "dockerode": "4.0.7",
    "hono": "4.9.5",
    "js-yaml": "4.1.0",
    "node-forge": "1.3.1",
    "pino": "9.9.0",
    "pino-pretty": "13.1.1",
    "semver": "7.7.2",
    "tar-stream": "3.1.7",
    "typed-rest-client": "2.1.0",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@types/dockerode": "3.3.43",
    "@types/js-yaml": "4.0.9",
    "@types/node-forge": "1.3.14",
    "@types/semver": "7.7.0",
    "@types/tar-stream": "3.1.4"
  }
}
