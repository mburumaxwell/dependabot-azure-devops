{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/crons/cleanup/database",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/crons/trigger-update-jobs",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/crons/trigger-sync-projects",
      "schedule": "23 */6 * * *"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "dependabot/*": false
    }
  },
  "github": {
    "autoJobCancelation": false
  }
}
