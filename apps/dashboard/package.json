    "predev": "pnpm prisma-gen",
    "typegen": "next typegen",
    "build": "next build",
    "dev": "next dev --port 3003",
    "start": "next start",
    "lint": "biome check",
    "test": "vitest",
    "clean": "rimraf .next out .react-email"
